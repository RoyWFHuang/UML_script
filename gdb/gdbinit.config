python gdb.COMPLETE_EXPRESSION = gdb.COMPLETE_SYMBOL

add-auto-load-safe-path scripts/gdb/
file vmlinux
lx-version
set args umid=uml0 hostname=uml1 eth0=tuntap,tap0 root=/dev/root rootfstype=hostfs rootflags=FULLPATH/rootfs rw mem=64M init=/init console=tty0 quiet
handle SIGSEGV nostop noprint
handle SIGUSR1 nopass stop print
